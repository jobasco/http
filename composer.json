{
  "name": "jobasco/http",
  "description": "PSR HTTP Message implementations",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "http"
  ],
  "homepage": "https://github.com/jobasco/http",
  "authors": [
    {
      "name": "Joseph Asamoah",
      "email": "asamoahjoseph@gmail.com",
      "homepage": "https://github.com/jobasco"
    }
  ],
  "support": {
    "issues": "https://github.com/jobasco/http/issues",
    "source": "https://github.com/jobasco/http"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev",
      "dev-develop": "1.1-dev"
    }
  },
  "require": {
    "php": "^7.2 || ^8.0",
    "psr/http-message": "2.0"
  },
  "require-dev": {
    "phpunit/PHPUnit": "11.0",
    "squizlabs/php_codesniffer": "3.7"
  },
  "provide": {
    "psr/http-message-implementation": "2.0.0"
  },
  "autoload": {
    "psr-4": {
      "jobasco\\Http\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhlyTest\\Http\\": "test/"
    },
    "files": [
      "test/TestAsset/Functions.php",
      "test/TestAsset/SapiResponse.php"
    ]
  }
}
